{
  "cache": {
    "cacheLocation": "./.cache/cspell",
    "useCache": true
  },
  "files": ["**/*.js", "**/*.json", "**/*.md", "source/**/*", "static/**/*", "tests/**/*"],
  "ignorePaths": ["build/**/*"],
  "ignoreRegExpList": [
    "\"tstyche/tstyche\"",
    "tstyche#\\d+",
    "tstyche[.]config[.]json"
  ],
  "language": "en-US",
  "useGitignore": true,
  "words": ["mochajs"]
}
